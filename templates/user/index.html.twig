{% extends 'base.html.twig' %}

{% block title %}User index{% endblock %}

{% block body %}
    <h1>User index</h1>


        {% for user in users %}


                <p>{{ user.id }}</p>
                <p>{{ user.username }}</p>
                <p>
                    <a href="{{ path('app_user_show', {'id': user.id}) }}">show</a>
                    <a href="{{ path('app_user_edit', {'id': user.id}) }}">edit</a>
                </p>

        {% else %}

                <p colspan="5">no records found</p>

        {% endfor %}

{% endblock %}
